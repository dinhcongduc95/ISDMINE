@model WebApplication1.Models.TestResult

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>

<div>
    <h4>TestResult</h4>
    <hr />
    <dl class="dl-horizontal">

        <dt>
            @Html.DisplayNameFor(model => model.User)
        </dt>

        <dd>
            @((Model.User != null) ? Model.User.UserName : "")
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Test)
        </dt>

        <dd>
            @((Model.Test != null) ? Model.Test.Name : "")
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Mark)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Mark)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Passed)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Passed)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.TestResultId }) |
    @Html.ActionLink("Back to List", "Index")
</p>
